    ],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "($tab_mode + '_' + $tab_name + '_3page_view')",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },

  "recipe_book_3page_content@recipe_book_page_content_panel": {
    "$scroll_position": [ 0, -200],
