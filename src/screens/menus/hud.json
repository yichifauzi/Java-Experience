{
  "namespace": "hud_mcjava",

  "hud_screen_content_panel": {
    "type": "panel",
    "controls": [
      { "root@hud_mcjava.root_content_panel": {} },
      { "camera@hud_mcjava.camera_renderer": {} },
      { "inventory_selected_icon_button@container_mcjava.inventory_selected_icon_button": {} }
    ]
  },

  "root_content_panel": {
    "type": "panel",
    "controls": [
      { "left_helpers@helper_mcjava.hud_left_helpers": {} },
      { "right_helpers@helper_mcjava.hud_right_helpers": {} },
      { "centered_gui_elements@hud_mcjava.centered_gui_elements": {} },
      { "classic_hotbar_zone_content_panel@hud_mcjava.hotbar_zone_content_panel": {} },
      {
        "centered_ridingvr_gui_elements@hud_mcjava.hotbar_zone_content_panel": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [0, 60],
          "size": [182, 50],
          "bindings": [
            {
              "binding_name": "#hud_visible_centered_ridingvr",
              "binding_name_override": "#visible",
              "binding_type": "global"
            }
          ]
        }
      },
      { "pocket_hotbar_zone_content_panel@hud_mcjava.pocket_hotbar_zone_content_panel": {} },
      {
        "gamertag_label_for_splitscreen": {
          "type": "label",
          "size": [ "default", "default" ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [-4, 4],
          "color": "$hud_gamertag_color",
          "text_alignment": "right",
          "text": "#gamertag",
          "bindings": [
            {
              "binding_name": "#gamertag"
            }
          ]
        }
      },
      {
        "hud_tip_text_factory": {
          "type": "factory",
          "control_name": "@hud.hud_tip_text"
        }
      },
      {
        "hud_actionbar_text_area": {
          "type": "panel",
          "factory": {
            "name": "hud_actionbar_text_factory",
            "control_ids": {
              "hud_actionbar_text": "hud_actionbar_text@hud.hud_actionbar_text"
            }
          }
        }
      },
      {
        "hud_title_text_area": {
          "type": "panel",
          "factory": {
            "name": "hud_title_text_factory",
            "control_ids": {
              "hud_title_text": "hud_title_text@hud.hud_title_text"
            }
          }
        }
      },
      {
        "mob_effects_renderer@hud.mob_effects_renderer": {
          "size": [ "100%", "75%" ],
          "offset": [0, 0],
          "anchor_from": "center",
          "anchor_to": "center"
        }
      },
      { "vignette_rend@hud.vignette_renderer": {} },
      { "cursor_rend@hud.cursor_renderer": {} },
      {
        "game_tip@game_tip.game_tip_panel": {
          "$offset": [ 0, "-90px" ],
          "variables": [
            {
              "requires": "$pocket_screen",
              "$offset": [ 0, "-60px" ]
            }
          ],
          "offset": "$offset"
        }
      },
      { "sidebar@scoreboard.scoreboard_sidebar": {} },
      {
        "progress_rend@hud.progress_indicator_renderer": {
          "offset": [ "50%", "50%" ]
        }
      },
      { "player_position@hud_mcjava.player_position": {} },
      { "chat_panel@hud_mcjava.chat_panel": {} },
      {
        "boss_health_panel": {
          "type": "stack_panel",
          "orientation": "vertical",
          "size": [ "100%", "100%" ],
          "controls": [
            {
              "boss_hud_panel": {
                "type": "panel",
                "controls": [
                  { "boss_health_grid@hud_mcjava.boss_health_grid": {} }
                ]
              }
            }
          ]
        }
      },
      { "save_icon@auto_save": {} }
    ],
    "bindings": [
      {
        "binding_name": "#hud_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      },
      {
        "binding_name": "#hud_alpha",
        "binding_name_override": "#alpha",
        "binding_type": "global"
      },
      {
        "binding_name": "#hud_propagate_alpha",
        "binding_name_override": "#propagateAlpha",
        "binding_type": "global"
      }
    ]
  },

  /* Auto Save */

  "auto_save": {
    "type": "image",
    "layer": 31,
    "texture": "textures/ui/auto_save",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [ -15, 15 ],
    "size": [ 18, 30 ],
    "uv_size": [ 18, 30 ],
    "uv": "@hud_mcjava.auto_save_animation",
    "disable_anim_fast_forward": true,
    "bindings": [
      {
        "binding_name": "#auto_save_animation_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "auto_save_animation": {
    "anim_type": "flip_book",
    "initial_uv": [ 0, 0 ],
    "frame_count": 9,
    "frame_step": 18,
    "fps": 10,
    "reversible": true,
    "easing": "linear"
  },

  /* Player Renderer */

  "centered_gui_elements": {
    "type": "panel",
    "$hud_player_rend_desktop_offset|default": [ 15, 15 ],
    "variables": [
      {
        "requires": "($is_holographic and not $is_living_room_mode)",
        "$hud_player_rend_desktop_offset": [ 100, 50 ]
      }
    ],
    "controls": [
      {
        "hud_player_rend_desktop@hud.hud_player_renderer": {
          "offset": "$hud_player_rend_desktop_offset",
          "anchor_from": "top_left",
          "anchor_to": "top_left"
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#hud_visible_centered_gui_elements",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },

  /* Chat */

  "chat_panel": {
    "type": "panel",
    "clips_children": true,
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "size": [ 326, 180],
    "offset": [0, -40],
    "controls": [
      {
        "stack_panel": {
          "type": "stack_panel",
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [0, -1],
          "factory": {
            "name": "chat_item_factory",
            "max_children_size": 50,
            "control_ids": {
              "chat_item": "chat_item@hud_mcjava.chat_grid_item"
            }
          }
        }
      }
    ]
  },

  "chat_grid_item": {
    "type": "panel",
    "layer": 1,
    "size": [ "100%", "100%c" ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "controls": [
      {
        "chat_background": {
          "type": "image",
          "texture": "textures/ui/Black",
          "alpha": 0.5,
          "size": [ "100%", "100%c - 1px" ],
          "anims": [
            "@hud_mcjava.anim_chat_bg_wait"
          ],
          "controls": [
            {
              "chat_text@hud_mcjava.chat_label": {
                "offset": [ 2, 1 ]
              }
            }
          ]
        }
      }
    ]
  },

  "chat_label": {
    "type": "label",
    "layer": 31,
    "text": "#text",
    "size": [ "100% - 2px", "default" ],
    "shadow": true,
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "localize": false,
    "color": "$chat_text_color",
    "font_type": "$chat_font_type",
    "font_scale_factor": "$chat_font_scale_factor",
    "anims": [
      "@hud_mcjava.anim_chat_txt_wait"
    ],
    "bindings": [
      {
        "binding_name": "#chat_text",
        "binding_name_override": "#text",
        "binding_type": "collection",
        "binding_collection_name": "chat_text_grid",
        "binding_condition": "once"
      }
    ]
  },

  "anim_chat_txt_alpha": {
    "anim_type": "alpha",
    "easing": "in_quart",
    "duration": 1,
    "from": 1,
    "to": 0
  },

  "anim_chat_txt_wait": {
    "anim_type": "wait",
    "duration": "$chat_item_lifetime",
    "next": "@hud_mcjava.anim_chat_txt_alpha"
  },

  "anim_chat_bg_alpha": {
    "anim_type": "alpha",
    "easing": "in_quart",
    "destroy_at_end": "chat_grid_item",
    "duration": 1,
    "from": 0.5,
    "to": 0
  },

  "anim_chat_bg_wait": {
    "anim_type": "wait",
    "duration": "$chat_item_lifetime",
    "next": "@hud_mcjava.anim_chat_bg_alpha"
  },

  /* Hotbar Zone Content */

  "hotbar_zone_content_panel": {
