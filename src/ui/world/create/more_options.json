{
  "namespace": "more_create_options_mcjava",

  "more_create_options_screen_content_panel": {
    "type": "panel",
    "$anchor_from": "top_middle",
    "$anchor_to": "top_middle",
    "$is_world_edit|default": false,
    "controls": [
      {
        "title@screen_mcjava.title": {
          "offset": [0, 19],
          "text": "selectWorld.create"
        }
      },
      { "world_name@more_create_options_mcjava.world_seed_box": { "offset": [0, 46] } },
      { "world_name@more_create_options_mcjava.edu_world_seed_box": { "offset": [0, 46] } },
      { "seed_info_panel@more_create_options_mcjava.seed_info_panel": { "offset": [55, 84] } },
      { "experimental_gameplay@more_create_options_mcjava.experimental_gameplay_toggle": { "offset": [-80, 100] } },
      { "no_cheats_world_type@more_create_options_mcjava.no_cheats_world_type_panel": { "offset": [80, 100] } },
      { "cheats_enabled_world_type@more_create_options_mcjava.cheats_enabled_world_type_panel": { "offset": [80, 100] } },
      { "experimental_gameplay_info@more_create_options_mcjava.experimental_gameplay_info_panel": { "offset": [4, 121] } },
      { "allow_cheats@more_create_options_mcjava.allow_cheats_toggle": { "offset": [-80, 151] } },
      { "game_settings@more_create_options_mcjava.bonus_chest_toggle": { "offset": [80, 151] } },
      { "allow_cheats_info@more_create_options_mcjava.allow_cheats_info_panel": { "offset": [1, 171] } },
      { "game_settings@more_create_options_mcjava.bonus_map_toggle": { "offset": [-80, 185] } },
      { "back@settings_mcjava.back_button": { "anchor_from": "top_middle", "anchor_to": "top_middle", "size": [150, 20], "$toggle_button_name": "more_create_options", "offset": [80, 185] } },
      {
        "create_world@world_create_mcjava.create_world_button": {
          "ignored": "$is_world_edit",
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "offset": [-80, -8]
        }
      },
      {
        "cancel@screen_mcjava.exit_button": {
          "ignored": "$is_world_edit",
          "$button_text": "gui.cancel",
          "size": [150, 20],
          "offset": [80, -8],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle"
        }
      }
    ]
  },

  "world_seed_box@text_box_mcjava.text_box_with_label_and_button": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$tts_section_header": "createWorldScreen.worldPreferences",
    "ignored": "($education_edition or not $is_seeds_enabled)",
    "$box_label": "selectWorld.enterSeed",
    "$option_binding_name": "#world_seed",
    "$option_enabled_binding_name": "#world_seed_enabled",
    "$text_box_name": "world_seed_text_box",
    "$pressed_button_name": "choose_ugc_level_seed",
    "$button_tts_header": "accessibility.settings.chooseSeed"
  },

  "level_seed_selector_trial@world_section.option_text_edit_mock_with_button": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$input_type": "option_with_button",
    "$tts_section_header": "createWorldScreen.worldPreferences",
    "ignored": "($education_edition or $is_seeds_enabled)",
    "$box_label": "selectWorld.enterSeed",
    "$box_binding_name": "#world_seed",
    "$input_enabled_binding_name": "#world_seed_enabled",
    "$pressed_button_name": "button.redirect_to_upsell"
  },

  "edu_world_seed_box@text_box_mcjava.text_box_with_label": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$tts_section_header": "createWorldScreen.worldPreferences",
    "ignored": "(not $education_edition)",
    "$box_label": "selectWorld.enterSeed",
    "$option_binding_name": "#world_seed",
    "$option_enabled_binding_name": "#world_seed_enabled",
    "$text_box_name": "world_seed_text_box"
  },

  "experimental_gameplay_toggle@option_toggle_mcjava.option_toggle": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$tts_section_header": "createWorldScreen.worldSettings",
    "$option_toggle_text": "selectWorld.experimentalGameplay",
    "$option_binding_name": "#experimental_gameplay",
    "$option_enabled_binding_name": "#experimental_gameplay_enabled",
    "$toggle_name": "experimental_gameplay",
    "bindings": [
      {
        "binding_name": "#show_experimental_gameplay",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "no_cheats_world_type_panel@switch_option_mcjava.switch_option_panel": {
    "size": [150, 20],
    "$options_enabled": "#world_type_dropdown_enabled",
    "$switch_option_label": "selectWorld.mapType",
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      {
        "infinite@switch_option_mcjava.switch_option": {
          "$option_action_text": "generator.default",
          "$this_action": "#world_type_radio_infinite",
          "$next_action": "#world_type_radio_old"
        }
      },
      { "alt@more_create_options_mcjava.alt_flat_world_toggle_panel": {} },
      {
        "small@switch_option_mcjava.switch_option": {
          "$option_action_text": "generator.small",
          "$this_action": "#world_type_radio_old",
          "$next_action": "#world_type_radio_infinite"
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "(not #allow_cheats)",
        "binding_name_override": "#visible"
      }
    ]
  },

  "alt_flat_world_toggle_panel": {
    "type": "panel",
    "size": [150, 20],
    "controls": [
      {
        "alt_flat_world_toggle@toggle_mcjava.toggle": {
          "$state_checked_text": "generator.flat",
          "$state_unchecked_text": "generator.flat",
          "$option_binding_name": "#world_type_radio_old",
          "$option_enabled_binding_name": "#world_type_dropdown_enabled",
          "$toggle_name": "#world_type_radio_old"
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#world_type_radio_flat",
        "binding_name_override": "#visible"
      }
    ]
  },

  "cheats_enabled_world_type_panel@switch_option_mcjava.switch_option_panel": {
    "size": [150, 20],
    "$options_enabled": "#world_type_dropdown_enabled",
    "$switch_option_label": "selectWorld.mapType",
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      {
        "infinite@switch_option_mcjava.switch_option": {
          "$option_action_text": "generator.default",
          "$this_action": "#world_type_radio_infinite",
          "$next_action": "#world_type_radio_flat"
        }
      },
      {
        "flat@switch_option_mcjava.switch_option": {
          "$option_action_text": "generator.flat",
          "$this_action": "#world_type_radio_flat",
          "$next_action": "#world_type_radio_old"
        }
      },
      {
        "small@switch_option_mcjava.switch_option": {
          "$option_action_text": "generator.small",
          "$this_action": "#world_type_radio_old",
          "$next_action": "#world_type_radio_infinite"
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#allow_cheats",
        "binding_name_override": "#visible"
      }
    ]
  },

  "allow_cheats_toggle@option_toggle_mcjava.option_toggle": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$option_toggle_text": "selectWorld.allowCommands",
    "$option_binding_name": "#allow_cheats",
    "$option_enabled_binding_name": "#allow_cheats_enabled",
    "$toggle_name": "allow_cheats",
    "$toggle_binding_condition": "always_when_visible"
  },

  "info_panel": {
    "type": "panel",
    "size": [310, 10],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      {
        "info": {
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": "$info_offset",
          "$info_offset|default": [0, 1],
          "type": "label",
          "text": "$text",
          "color": [0.62, 0.62, 0.62],
          "shadow": true,
          "text_alignment": "left"
        }
      }
    ]
  },

  "seed_info_panel@more_create_options_mcjava.info_panel": {
    "$text": "selectWorld.seedInfo",
    "$info_offset": [0, 1]
  },

  "experimental_gameplay_info_panel@more_create_options_mcjava.info_panel": {
    "$text": "selectWorld.experimentalGameplay.info"
  },

  "allow_cheats_info_panel@more_create_options_mcjava.info_panel": {
    "$text": "selectWorld.allowCommands.info"
  },

  "bonus_map_toggle@option_toggle_mcjava.option_toggle": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$option_toggle_text": "createWorldScreen.startWithMap",
    "$option_binding_name": "#start_with_map",
    "$option_enabled_binding_name": "#start_with_map_enabled",
    "$toggle_name": "start_with_map"
  },

  "bonus_chest_toggle@option_toggle_mcjava.option_toggle": {
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$option_toggle_text": "createWorldScreen.bonusChest",
    "$option_binding_name": "#bonus_chest",
    "$option_enabled_binding_name": "#bonus_chest_enabled",
    "$toggle_name": "bonus_chest"
  }
}