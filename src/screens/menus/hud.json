      },
      {
        "horse_heart_rend_0@hud.horse_heart_renderer": {
          "offset": [ 180, -54 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "bindings": [
            {
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "horse_heart_rend_1@hud.horse_heart_renderer": {
          "offset": [ 180, -48 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "bindings": [
            {
              "binding_name": "#is_not_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "armor_rend@hud.armor_renderer": {
          "offset": [ -1, -38 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left"
        }
      },
      {
        "hunger_rend@hud.hunger_renderer": {
          "offset": [ 180, -38 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left"
        }
      },
      {
        "bubbles_rend_0@hud.bubbles_renderer": {
          "offset": [ 180, -48 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "bindings": [
            {
              "binding_name": "#is_not_riding",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "bubbles_rend_1@hud.bubbles_renderer": {
          "offset": [ 180, -68 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "bindings": [
            {
              "binding_name": "#is_riding",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "horse_jump_rend@hud.horse_jump_renderer": {
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "offset": [ 0, -28 ],
          "layer": 7,
          "bindings": [
            {
              "binding_name": "#is_riding",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      { "texts_panel@hud_mcjava.texts_panel": {} },
      { "xp_bar_and_score_content_panel@hud_mcjava.xp_bar_and_score_content_panel": {} },
      { "hotbar_content_panel@hud_mcjava.hotbar_content_panel": {} }
    ],
    "bindings": [
      {
        "binding_name": "#hud_visible_centered",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },

  "pocket_hotbar_zone_content_panel@hud_mcjava.hotbar_zone_content_panel": {
    "bindings": [
      {
        "binding_name": "#hud_visible_not_centered",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },

  "texts_panel": {
    "type": "panel",
    "size": [ "100%", 10 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "controls": [
      {
        "item_text_factory": {
          "size": [ "100%", 10 ],
          "type": "factory",
          "control_ids": {
            "survival_item_text": "@hud_mcjava.survival_item_name_text",
            "survival_item_text_above_interact_button": "@hud_mcjava.survival_above_interact_button_item_text",
            "survival_jukebox_text": "@hud_mcjava.survival_jukebox_popup_text",
            "survival_jukebox_text_above_interact_button": "@hud_mcjava.survival_jukebox_above_interact_button_popup_text",
            "creative_item_text": "@hud_mcjava.creative_item_name_text",
            "create_item_text_above_interact_button": "@hud_mcjava.creative_above_interact_button_item_text",
            "creative_jukebox_text": "@hud_mcjava.creative_jukebox_popup_text",
            "creative_jukebox_text_above_interact_button": "@hud_mcjava.creative_jukebox_above_interact_button_popup_text"
          }
        }
      }
    ]
  },

  "xp_bar_and_score_content_panel": {
    "type": "panel",
    "size": [182, 12],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": [0, -24],
    "controls": [
      { "xp_level_panel@hud_mcjava.xp_level_panel": {} },
      { "xp_bar_panel@hud_mcjava.xp_bar_panel": {} }
    ],
    "bindings": [
      {
        "binding_name": "#is_not_creative_mode",
        "binding_name_override": "#visible"
      }
    ]
  },

  "xp_level_panel": { 
    "type": "panel",
    "size": [182, 12],
    "layer": 5,
    "controls": [
      { "xp_level_label@hud_mcjava.xp_level_text": { "color": "$experience_bar_text_color", "layer": 5 } },
      { "xp_level_top_shadow@hud_mcjava.xp_level_text": { "offset": [0, -1] } },
      { "xp_level_left_shadow@hud_mcjava.xp_level_text": { "offset": [-1, 0] } },
      { "xp_level_right_shadow@hud_mcjava.xp_level_text": { "offset": [1, 0] } },
      { "xp_level_bottom_shadow@hud_mcjava.xp_level_text": { "offset": [0, 1] } }
    ]
  },

  "hotbar_content_panel": { 
    "type": "panel",
    "size": [182, 22],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "controls": [
      { "left_inv_button_panel@hud_mcjava.left_inv_button_panel": {} },
      { "hotbar_panel@hud_mcjava.hotbar_panel": {} },
      { "right_inv_button_panel@hud_mcjava.right_inv_button_panel": {} }
    ]
  },

  "hotbar_panel": { 
    "type": "panel",
    "size": [182, 22],
    "controls": [
      { "hotbar_bg@hud_mcjava.hotbar_bg": {} },
      { "hotbar_grid@hud_mcjava.hotbar_grid": {} }
    ]
  },

  /* For Touch Devices */

  "left_inv_button_panel": { 
    "type": "panel",
    "size": [22, 22],
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "offset": [-22, 0],
    "controls": [
      { "inventory@hud_mcjava.hotbar_inventory_button": {} }
    ],
    "bindings": [
      {
        "binding_name": "#hotbar_elipses_left_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },

  "right_inv_button_panel": { 
    "type": "panel",
    "size": [22, 22],
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "offset": [22, 0],
    "controls": [
      { "inventory@hud_mcjava.hotbar_inventory_button": {} }
    ],
    "bindings": [
      {
        "binding_name": "#hotbar_elipses_right_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },

  /* Content */

  /* Renderer's */

  "camera_renderer": {
    "type": "custom",
    "renderer": "camera_renderer",
    "layer": 1
  },

  /* Boss Health Bar */

  "boss_health_grid": {
    "type": "grid",
    "grid_dimension_binding": "#boss_grid_dimension",
    "grid_item_template": "hud_mcjava.boss_health_panel",
    "grid_fill_direction": "vertical",
    "collection_name": "boss_bars",
    "anchor_to": "top_middle",
    "anchor_from": "top_middle",
    "size": [ 182, "30%" ]
  },

  "boss_health_panel": {
    "type": "panel",
    "size": [ 182, 16 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$progress_bar_collection": "boss_bars",
    "controls": [
      { "boss_name@hud_mcjava.boss_name_panel": {} },
      { "progress_bar@hud_mcjava.boss_bar_panel": {} }
    ],
    "bindings": [
      {
        "binding_name": "#bar_visible",
        "binding_type": "collection",
        "binding_collection_name": "boss_bars",
        "binding_name_override": "#visible"
      }
    ]
  },

  "boss_name_panel": {
    "type": "panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [0, 2],
    "controls": [
      {
        "boss_name": {
          "type": "label",
          "color": "$boss_name_text_color",
          "text": "#bossName",
          "size": [ "default", "default"],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "shadow": true,
          "bindings": [
            {
              "binding_name": "#bossName",
              "binding_type": "collection",
              "binding_collection_name": "boss_bars"
            }
          ]
        }
      }
    ]
  },

  "boss_bar_panel": {
    "type": "panel",
    "size": [182, 5],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": [0, 1],
    "controls": [
      { "empty_progress_bar@hud_mcjava.empty_progress_bar": {} },
      { "filled_progress_bar@hud_mcjava.filled_progress_bar": { "layer": 3 } }
    ]
  },

  "empty_progress_bar@common_mcjava.image": {
    "type": "image",
    "texture": "assets/minecraft/textures/gui/bars",
    "size": [182, 5],
    "variables": "$boss_bar_empty_resol"
  },

  "filled_progress_bar@hud_mcjava.empty_progress_bar": {
    "variables": "$boss_bar_filled_resol",
    "clip_direction": "left",
    "clip_pixelperfect": true,
    "bindings": [
      {
        "binding_name": "#progress_percentage",
        "binding_name_override": "#clip_ratio",
        "binding_type": "collection",
        "binding_collection_name": "$progress_bar_collection"
      }
    ]
  },

  /* Player Coordinates X Y Z */

  "player_position": {
    "type": "image",
    "size": [ "100%c + 2px", "100%c - 1px" ],
    "texture": "src/assets/img/white",
    "color": [0.3, 0.3, 0.3],
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "offset": [-1, -1],
    "alpha": 0.4,
    "controls": [
      {
        "player_position_text": {
          "type": "label",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "layer": 1,
          "enable_profanity_filter": false,
          "color": "$chat_text_color",
          "text": "#text",
          "shadow": true,
          "bindings": [
            {
              "binding_name": "#player_position_text",
              "binding_name_override": "#text",
              "binding_condition": "always_when_visible",
              "binding_type": "global"
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#player_position_visible",
        "binding_name_override": "#visible",
        "binding_condition": "always",
        "binding_type": "global"
      }
    ]
  },

  /* Items and Jukebox Text */

  "item_name_text": {
    "type": "panel",
    "size": [ "100%", 10 ],
    "clips_children": true,
    "controls": [
      {
        "item_text_label": {
          "type": "label",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "text_alignment": "center",
          "layer": 31,
          "color": "$tool_tip_text",
          "text": "#text",
          "shadow": true,
          "alpha": "@hud.anim_item_name_text_alpha",
          "bindings": [
            {
              "binding_name": "#item_text",
              "binding_name_override": "#text"
            }
          ]
        }
      }
    ]
  },
  
  "jukebox_text": {
    "type": "panel",
    "size": [ "100%", 10 ],
