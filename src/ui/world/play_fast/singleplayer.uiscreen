          "controls": [
            {
              "local_world_name@play.local_world_name": {
                "size": ["100%", 10],
                "color": [1, 1, 1]
              }
            }
          ]
        }
      },
      { "@content.v2px": {} },
      {
        "top_panel": {
          "type": "stack_panel",
          "size": ["100%", 9],
          "orientation": "horizontal",
          "layer": 4,
          "controls": [
            {
              "local_world_name@play.local_world_name": {
                "size": ["default", 10],
