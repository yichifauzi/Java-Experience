  "bottom_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [310, 20],
    "offset": [0, -9],
    "controls": [
      { "reset@mouse_mcjava.reset_button": {} },
      { "fill@screen_mcjava.hfill": {} },
      { "back@settings_mcjava.back_button": { "size": [150, 20] } }
    ]
  },

  "mouse_settings_button@toggle_button_mcjava.toggle_button": {
    "$toggle_button_name": "mouse_settings",
    "size": [150, 20],
    "$toggle_index": 1,
    "$toggle_button_text": "options.mouse_settings",
    "$toggle_control_name": "mouse_settings_view"
  },

  "auto_jump_toggle@option_toggle_mcjava.option_toggle": {
    "$option_toggle_text": "options.autoJump",
    "$option_binding_name": "#mouse_autoJump",
    "$option_enabled_binding_name": "#mouse_autojump_enabled",
    "$toggle_name": "mouse_autojump"
  },

  "mouse_keymapping_item": {
    "type": "panel",
    "layer": 1,
    "size": ["100%", 20],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "option_info_label_control": {
          "type": "panel",
          "size": ["100%", "100%"],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "controls": [
            {
              "keymapping_label_control": {
                "type": "label",
                "color": "white",
                "size": ["100%", "default"],
                "text_alignment": "left",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "layer": 20,
                "text": "#keymapping_name",
                "bindings": [
                  {
                    "binding_name": "#keymapping_name",
                    "binding_type": "collection",
                    "binding_collection_name": "$keymapping_collection"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "keymapping_button_0@binding_button": {
          "$button_tts_header": "#audible_keymapping_name",
          "anchor_from": "top_right",
          "offset": [-88.5, 0],
          "anchor_to": "top_right",
          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "$keymapping_collection"
            },
            {
              "binding_name": "#audible_keymapping_name",
              "binding_type": "collection",
              "binding_collection_name": "$keymapping_collection"
            }
          ]
        }
      },
      {
        "keymapping_button_1@mouse_mcjava.reset_binding_button": {
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [-27.5, 0],
          "$pressed_button_name": "button.reset_binding",
          "$button_tts_header": "accessibility.settings.reset"
        }
      }/* ,
      {
        "2@reset_binding_button_locked": {
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [-27.5, 0]
        }
      } */
    ]
  },

  "binding_button@button_mcjava.button": {
    "size": [75, 20],
    "$pressed_button_name": "button.binding_button",
    "$button_type_panel": "mouse_mcjava.binding_button_content",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$keymapping_collection"
      }
    ]
  },

  "binding_button_content": {
    "type": "panel",
    "controls": [
      {
        "default_label": {
          "type": "label",
          "text": "#binding_button_text",
          "tts_override_control_value": " ",
          "color": "$text_color",
          "shadow": true,
          "bindings": [
            {
              "binding_name": "#binding_button_text",
              "binding_condition": "none",
              "binding_type": "collection",
              "binding_collection_name": "$keymapping_collection"
            }
          ]
        }
      }
    ]
  },

  "reset_binding_button@button_mcjava.button": {
    "layer": 1,
    "size": [50, 20],
    "$button_text": "controls.reset",
    "$pressed_button_name": "button.reset_binding",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$keymapping_collection"
      },
      {
        "ignored": "(not $better_mouse_keys_grid)",
        "binding_name": "$bindName",
        "binding_condition": "none",
        "binding_type": "collection",
        "binding_collection_name": "$keymapping_collection",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "reset_binding_button_locked@screen_mcjava.locked": {
    "type": "input_panel",
    "focus_enabled": false,
    "layer": 10,
    "$button_text": "Reset",
    "$text_locked_color": [0.62, 0.62, 0.62],
    "size": [50, 20],
    "bindings": [
      {
        "binding_name": "$bindName",
        "binding_condition": "none",
        "binding_type": "collection",
        "binding_collection_name": "$keymapping_collection",
