  /* Content */

  "title_text@screen_mcjava.title": {
    "offset": [ 0, 10 ],
    "text": "Structure Block"
  },

  "structure_button": {
    "type": "panel",
    "size": [150, 20],
    "controls": [
      {
        "show_structure_button@show_structure_button": {
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "structure_view",
              "source_property_name": "(not #toggle_state)",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "hide_structure_button@hide_structure_button": {
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "structure_view",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ]
        }
      }
    ]
  },

  "show_structure_button@toggle_button_mcjava.toggle_button": {
    "size": [150, 20],
    "$toggle_button_name": "structure",
    "$toggle_index": 1,
    "$toggle_button_text": "Show Structure",
    "$toggle_control_name": "structure_view"
  },

  "hide_structure_button@toggle_button_mcjava.toggle_button": {
    "size": [150, 20],
    "$toggle_button_name": "structure",
    "$toggle_index": 0,
    "$toggle_button_text": "Hide Structure",
    "$toggle_control_name": "structure_view"
  },

  "mode_panel@switch_option_mcjava.switch_option_panel": {
    "size": [50, 20],
    "$options_enabled": "#mode_dropdown_enabled",
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "offset": [0, -25],
    "$options": [
      {
        "save@switch_option_mcjava.switch_option": {
          "size": [50, 20],
          "$switch_option_label": "Save",
          "$this_action": "#structure_save_mode",
          "$next_action": "#structure_load_mode"
        }
      },
      {
        "load@switch_option_mcjava.switch_option": {
          "ignored": "(not $data_support and not $3DExport_support)",
          "size": [50, 20],
          "$switch_option_label": "Load",
          "$this_action": "#structure_load_mode",
          "$next_action|default": "#structure_data_mode"
        }
      },
      {
        "load@switch_option_mcjava.switch_option": {
          "ignored": "(not $data_support)",
          "size": [50, 20],
          "$switch_option_label": "Load",
          "$this_action": "#structure_load_mode",
          "$next_action|default": "#structure_data_mode"
        }
      },
      {
        "load@switch_option_mcjava.switch_option": {
          "ignored": "$data_support",
          "size": [50, 20],
          "$switch_option_label": "Load",
          "$this_action": "#structure_load_mode",
          "$next_action|default": "#structure_3d_export_mode"
        }
      },
      {
        "data@switch_option_mcjava.switch_option": {
          "size": [50, 20],
          "ignored": "(not $data_support)",
          "$switch_option_label": "Data",
          "$this_action": "#structure_data_mode",
          "$next_action|default": "#structure_3d_export_mode"
        }
      },
      {
        "data@switch_option_mcjava.switch_option": {
          "size": [50, 20],
          "ignored": "$data_support",
          "$switch_option_label": "Data",
          "$this_action": "#structure_data_mode",
          "$next_action|default": "#structure_save_mode"
        }
      },
      {
        "hard@switch_option_mcjava.switch_option": {
          "size": [50, 20],
          "ignored": "(not $3DExport_support)",
          "$switch_option_label": "3D Export",
          "$this_action": "#structure_3d_export_mode",
          "$next_action": "#structure_save_mode"
        }
      }
    ]
  },

  "text_edit_control@text_box_mcjava.text_box": {
    "property_bag": {
      "#property_field": "#item_name"
    },
    "$placeholder_text": "0"
  },

