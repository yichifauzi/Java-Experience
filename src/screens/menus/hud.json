          "text_alignment": "center",
          "layer": 31,
          "text": "#text",
          "alpha": "@hud.anim_item_name_text_alpha",
          "bindings": [
            {
              "binding_name": "#jukebox_text",
              "binding_name_override": "#text"
            }
          ]
        }
      }, */
      {
        "text_panel": {
          "type": "panel",
          "size": [ "100%", 10 ],
          "alpha": "@hud_mcjava.anim_item_name_text_alpha",
          "anims": [
            "@hud_mcjava.anim_item_name_text_alpha"
          ],
          "controls": [
            { "jukebox_text_0@jukebox_text_0": {} },
            { "jukebox_text_1@jukebox_text_1": {} },
            { "jukebox_text_0@jukebox_text_2": {} },
            { "jukebox_text_1@jukebox_text_3": {} },
            { "jukebox_text_0@jukebox_text_4": {} },
            { "jukebox_text_1@jukebox_text_5": {} },
            { "jukebox_text_0@jukebox_text_6": {} },
            { "jukebox_text_1@jukebox_text_7": {} },
            { "jukebox_text_0@jukebox_text_8": {} }/* ,
