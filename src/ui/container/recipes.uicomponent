    "$right_button_name": "($tab_mode + '_' + $tab_name + '_page2')",
    "$right_control_name": "($tab_mode + '_' + $tab_name + '_3page_view')",
    "controls": [ { "@recipes_ui.page_content_panel": {} } ],
