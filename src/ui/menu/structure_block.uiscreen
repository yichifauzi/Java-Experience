  "structure_block_content_panel": {
    "type": "panel",
    "size": [308, 202],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [0, 28],
    "controls": [
      { "console_title@command_block_mcjava.base_label": { "$text": "Structure Name" } },
      { "structure_name_wrapper@structure_block_mcjava.structure_name_wrapper": {} },
      { "show_bounding_box_panel@structure_block_mcjava.show_bounding_box_panel": {} },
      { "remove_blocks_panel@structure_block_mcjava.remove_blocks_panel": {} },
