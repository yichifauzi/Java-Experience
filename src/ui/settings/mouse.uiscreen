{
  "namespace": "mouse_mcjava",

  "mouse_content_panel": {
    "type": "panel",
    "controls": [
      {
        "mouse_content_panel@screen_mcjava.scroll_screen": {
          "$top_content": "mouse_mcjava.top_content",
          "$top_content_size": ["100%", 44],
          "$scrolling_content": "mouse_mcjava.better_keymapping_content",
          "$bottom_content": "mouse_mcjava.bottom_content",
          "variables": [
            {
              "requires": "(not $better_mouse_keys_grid)",
              "$scrolling_content": "mouse_mcjava.faster_keymapping_content"
            }
          ],
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "mouse_settings_view",
              "source_property_name": "(not #toggle_state)",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "mouse_settings@mouse_settings_mcjava.mouse_settings_content_panel": {
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "mouse_settings_view",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ]
        }
      }
    ]
  },
  
  "top_content": {
    "type": "panel",
    "controls": [
      { "title@screen_mcjava.title": { "offset": [0, 7], "text": "options.controls.title" } },
      {
        "stack_zero": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "size": [310, 20],
          "offset": [0, 18],
          "controls": [
            { "mouse_settings@mouse_mcjava.mouse_settings_button": {} },
            { "fill@screen_mcjava.hfill": {} },
            { "auto_jump@mouse_mcjava.auto_jump_toggle": { "size": [150, 20] } }
          ]
        }
      }
    ]
  },

  "faster_keymapping_content": {
    "type": "panel",
    "size": [318, "100%c"],
    "controls": [
      { "space_top@screen_mcjava.vspace": {} },
      {
        "keyboard_standart_options": {
          "type": "stack_panel",
          "size": ["100%", "100%c"],
          "$keymapping_grid_dimension": "#keyboard_standard_grid_dimension",
          "$keymapping_collection": "keyboard_standard_collection",
          "bindings": [
            {
              "binding_name": "#keyboard_show_standard_keyboard_options",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [ { "grid@keymapping_grid": {} } ]
        }
      },
      {
        "keyboard_standart_options": {
          "type": "stack_panel",
          "size": ["100%", "100%c"],
          "$keymapping_grid_dimension": "#keyboard_full_grid_dimension",
          "$keymapping_collection": "keyboard_full_collection",
          "bindings": [
            {
              "binding_name": "#keyboard_show_full_keyboard_options",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [ { "grid@full_keymapping_grid": {} } ]
        }
      },
      { "space_bottom@screen_mcjava.vspace": {} }
    ]
  },

  "keymapping_grid": {
    "type": "grid",
    "size": ["100%", "default"],
    "layer": 1,
    "grid_item_template": "mouse_mcjava.mouse_keymapping_item",
    "grid_dimension_binding": "$keymapping_grid_dimension",
    "collection_name": "$keymapping_collection",
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "bindings": [ { "binding_name": "$keymapping_grid_dimension" } ]
  },

  "full_keymapping_grid@keymapping_grid": {
    "$keymapping_grid_dimension": "#keyboard_full_grid_dimension",
    "$keymapping_collection": "keyboard_full_collection"
  },

