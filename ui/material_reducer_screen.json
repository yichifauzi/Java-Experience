{
  "namespace": "matreduce",
  
  "material_reducer@content.inventory_screen_common": {
    "$close_on_player_hurt|default": false,
    "close_on_player_hurt": "$close_on_player_hurt",
    "$screen_matrix": "material_reducer_mcjava.material_reducer_screen_content_panel",
    "$screen_background": "common.screen_background",
    "force_render_below": "$force_render_below",
    "variables": [
      {
        "requires": "$desktop_screen",
        "$screen_content": "material_reducer_mcjava.material_reducer_screen_content_panel"
      },
      {
        "requires": "$pocket_screen",
        "$screen_content": "material_reducer_mcjava.material_reducer_screen_content_panel"
      }
    ]
  }
}