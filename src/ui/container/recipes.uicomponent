{
  "namespace": "recipes_ui",

  /* Recipe Book Toggle Button */

  "recipe_book_button": {
    "type": "panel",
    "size": [20, 18],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 10,
    "$unchecked_toggle_bg": "resources0.recipe_button_default",
    "$checked_toggle_bg": "resources0.recipe_button_default",
    "$unchecked_hover_toggle_bg": "resources0.recipe_button_hover",
    "$checked_hover_toggle_bg": "resources0.recipe_button_hover",
    "$unchecked_locked_toggle_bg": "resources0.recipe_button_default",
    "$checked_locked_toggle_bg": "resources0.recipe_button_default",
    "$toggle_button_name": "layout_toggle",
    "$toggle_type_panel": "",
    "$toggle_binding_type": "global",
    "$use_controller_mapping": true,
    "$controller_mapping_from_id": "button.controller_back",
    "$controller_mapping_to_id": "button.menu_select",
    "controls": [
      {
        "gamepad_helper@gamepadhelper.gamepad_helper_rb_button": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": "$helper_offset",
          "$helper_offset|default": [0, -24]
        }
      },
      {
        "hide@toggle_button_ui.toggle_button": {
          "size": [20, 18],
          "$toggle_index": "$recipe_book_layout_index",
          "$toggle_state_binding_name": "#is_recipe_book_layout",
          "$focus_id": "recipe_book_layout_toggle",
          "bindings": [
            {
              "binding_name": "#is_survival_layout",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "show@toggle_button_ui.toggle_button": {
          "size": [20, 18],
          "$toggle_index": "$survival_layout_index",
          "$toggle_state_binding_name": "#is_survival_layout",
          "$focus_id": "recipe_book_layout_toggle",
          "bindings": [
            {
              "binding_name": "#is_recipe_book_layout",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  /* Common Tab */

  "left_tab@tab_ui.tab": {
    "size": [35, 26],
    "$toggle_focus_enabled": false,
    "$tab_name": "navigation_tab",
    "$toggle_default_selected": "$search_index",
    "$tab_type_panel": "recipes_ui.tab_with_item_renderer_panel",
    "$unchecked_tab_bg": "resources.recipe_book_tab_unchecked",
    "$checked_tab_bg": "resources.recipe_book_tab_checked",
    "$unchecked_hover_tab_bg": "resources.recipe_book_tab_unchecked",
    "$checked_hover_tab_bg": "resources.recipe_book_tab_checked"
  },

  "item_renderer": {
    "type": "custom",
    "renderer": "inventory_item_renderer",
    "size": [16, 16],
    "layer": 10
  },

  "tab_with_item_renderer_panel": {
    "type": "panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ 16, 16 ],
    "layer": 10,
    "$item_offset|default": [2, 0],
    "$left_item_offset|default": [-4, 0],
    "$right_item_offset|default": [6.5, 0],
    "$item_id_aux|default": 0,
    "$item_right_id_aux|default": 0,
    "$item_left_id_aux|default": 0,
    "variables": [
      {
        "requires": "($state = checked or $state = checked_hover or $state = checked_locked or $state = checked_locked_hover)",
        "$item_offset": [0, 0],
        "$left_item_offset": [-6, 0],
        "$right_item_offset": [4.5, 0]
      }
    ],
    "controls": [
      {
        "single@recipes_ui.item_renderer": {
          "anchor_from": "center",
          "anchor_to": "center",
          "offset": "$item_offset",
          "layer": 11,
          "property_bag": {
            "#item_id_aux": "$item_id_aux"
          }
        }
      },
      {
        "left@recipes_ui.item_renderer": {
          "anchor_from": "center",
          "anchor_to": "center",
          "offset": "$left_item_offset",
          "property_bag": { "#item_id_aux": "$item_left_id_aux" }
        }
      },
      {
        "right@recipes_ui.item_renderer": {
          "anchor_from": "center",
          "anchor_to": "center",
          "offset": "$right_item_offset",
          "layer": 11,
          "property_bag": { "#item_id_aux": "$item_right_id_aux" }
        }
      }
    ]
  },

  /* RB Toolbar */

  "search_box@text_box_ui.text_box": {
    "size": [82, 16],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$default_box_bg": "common.empty_panel",
    "$hover_box_bg": "common.empty_panel",
    "$locked_box_bg": "common.empty_panel",
    "property_bag": { "#property_field": "#item_name" },
    "$text_indent_size": ["100% - 2px", "100% - 2px"],
    "$text_indent_offset": [1.5, 0],
    "max_length": 30,
    "$placeholder_text": "gui.recipebook.search_hint",
    "$text_edit_binding_name": "#text_box_item_name",
    "$text_size": [ "default", 10 ],
    "$text_edit_box_hovered_button_id": "button.search_bar_hovered",
    "$text_edit_box_clear_to_button_id": "button.search_bar_clear",
    "$text_edit_box_selected_to_button_id": "button.search_bar_selected",
    "$text_edit_box_deselected_to_button_id": "button.search_bar_deselected",
    "focus_wrap_enabled": false,
    "$focus_id": "recipe_search_bar",
    "$focus_override_right": "craftable_toggle",
    "bindings": [
      {
        "binding_name": "#is_left_tab_search",
        "binding_name_override": "#visible"
      }
    ]
  },

  "filter_toggle_holder": {
    "type": "panel",
    "size": [26, 16],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [-11, 11],
    "bindings": [
      {
        "binding_name": "(not #is_creative_mode)",
        "binding_name_override": "#visible"
      }
    ],
    "controls": [ { "filter_toggle@recipes_ui.filter_toggle": {} } ]
  },

  "filter_toggle@switch_toggle_ui.switch_toggle": {
    "size": [ 26, 16 ],
    "$toggle_name": "toggle.enableFiltering",
    "$toggle_binding_type": "global",
    "$toggle_state_binding_name": "#filtering_enabled",
    "$unchecked_toggle_bg": "resources0.crafting_filter_unchecked",
    "$checked_toggle_bg": "resources0.crafting_filter_checked",
    "$unchecked_hover_toggle_bg": "resources0.crafting_filter_unchecked_hover",
    "$checked_hover_toggle_bg": "resources0.crafting_filter_checked_hover",
    "$unchecked_locked_toggle_bg": "resources0.crafting_filter_unchecked",
    "$checked_locked_toggle_bg": "resources0.crafting_filter_checked",
    "$toggle_type_panel": "",
    "$tooltip_name": "gui.recipebook.toggleRecipes.all",
    "$tooltip_variables": [
      {
        "requires": "($state = checked_locked_hover or $state = checked_hover)",
        "$tooltip_name": "gui.recipebook.toggleRecipes.craftable"
      }
    ],
    "$button_mappings": [
      {
        "to_button_id": "button.filter_toggle_hovered",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.menu_select",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.menu_ok",
        "mapping_type": "focused"
      },
      {
        "ignored": "(not $enable_directional_toggling)",
        "from_button_id": "button.menu_left",
        "to_button_id": "$toggle_off_button",
        "mapping_type": "pressed",
        "input_mode_condition": "gamepad_and_not_gaze"
      },
      {
        "ignored": "(not $enable_directional_toggling)",
        "from_button_id": "button.menu_right",
        "to_button_id": "$toggle_on_button",
        "mapping_type": "pressed",
        "input_mode_condition": "gamepad_and_not_gaze"
      }
    ]
  },

  /* Recipe Book Scrolling */

  "recipe_book_scroll_panel": {
    "type": "panel",
    "size": [128, 100],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [0, 30],
    "controls": [
      {
        "recipe_book_scroll_panel@recipes_ui.scroll_panel": {
          "$scroll_bar_offset": ["50% - 1.5px", 0 ],
          "$scrolling_panel_anchor": "top_left",
          "$scrolling_content": "recipes_ui.scroll_grid_panel",
          "$collection_name": "recipe_book"
        }
      }
    ]
  },

  "scroll_panel@container_ui.container_scroll_panel": {
    "$scroll_bg": "common.empty_panel",
    "$scroll_bar_size": [3, "100%"],
    "$scrolling_panel_base_button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.destroy_selection",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_secondary_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.destroy_selection",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.clear_hotbar_or_remove_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      }
    ]
  },

  "scroll_grid_panel@crafting.crafting_root_panel": {
    "type": "input_panel",
    "size": [125, "100%c" ],
    "controls": [ { "grid@recipes_ui.scroll_grid": {} } ]
  },

  "scroll_grid": {
    "type": "grid",
    "size": [ "100%", "default" ],
    "layer": 1,
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "$binding_condition|default": "visible",
    "$grid_item_precache_count|default": 0,
    "$grid_item_template|default": "recipes_ui.grid_item_for_recipe_book",
    "collection_name": "$collection_name",
    "grid_rescaling_type": "horizontal",
    "grid_item_template": "$grid_item_template",
    "bindings": [
      {
        "binding_name": "#recipe_book_total_items",
        "binding_name_override": "#maximum_grid_items",
        "binding_condition": "$binding_condition",
        "binding_type": "collection",
        "binding_collection_name": "$collection_name"
      }
    ]
  },

  "grid_item_for_recipe_book@crafting_ui.inventory_container_slot": {
    "$stack_count_required": false,
    "$durability_bar_required": false,
    "$item_renderer_binding_condition": "visible",
    "$button_ref": "recipes_ui.recipe_no_coalesce_container_slot_button",
    "$background_images": "recipes_ui.recipe_book_slot_backgrounds",
    "$slot_selected_binding_name": "#is_creative_selected_slot",
    "$stack_count_binding_name": "#recipe_craftable_count",
    "$item_collection_name": "recipe_book",
    "$highlight_slot_required": false,
    "$hover_text_binding_name": "#recipe_hover_text",
    "size": [25, 25]
  },

  "recipe_no_coalesce_container_slot_button@crafting_ui.no_coalesce_container_slot_button": {
    "$button_take_all_place_all": "button.recipe_select",
    "$button_take_half_place_one": "button.recipe_secondary",
    "$button_auto_place": "button.recipe_tertiary"
  },

  "recipe_book_slot_backgrounds": {
    "type": "image",
    "$cell_images_binding_condition|default": "visible",
    "size": [25, 25],
    "controls": [
      { "slot_default@recipes_ui.slot_recipe_default": {} },
      { "slot_collapsed@recipes_ui.slot_recipe_collapsed": {} },
      { "slot_expanded@recipes_ui.slot_recipe_expanded": {} },
      { "slot_group_item@recipes_ui.slot_recipe_item": {} },
      { "slot_selected@recipes_ui.slot_recipe_selected": {} },
      { "slot_red@recipes_ui.slot_recipe_red": {} },
      {
        "recipe_tip@game_tip.recipe_item_tip": {
          "bindings": [
            {
              "binding_name": "#recipe_search_tip_chevron_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "recipe_book"
            }
          ]
        }
      },
      {
        "recipe_tip1@game_tip.recipe_item_tip_box": {
          "bindings": [
            {
              "binding_name": "#recipe_search_tip_box_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "recipe_book"
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#container_item_background",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "$cell_images_binding_condition"
      }
    ]
  },

  "slot_recipe_default@resources0.rb_default_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 0)",
        "target_property_name": "#visible"
      }
    ]
  },

  "slot_recipe_collapsed@resources0.rb_group_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 1)",
        "target_property_name": "#visible"
      }
    ]
  },

  "slot_recipe_expanded@resources0.rb_group_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 2)",
        "target_property_name": "#visible"
      }
    ]
  },

  "slot_recipe_item@resources0.rb_default_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 3)",
        "target_property_name": "#visible"
      }
    ]
  },

  "slot_recipe_selected@resources0.rb_default_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 4)",
        "target_property_name": "#visible"
      }
    ]
  },

  "slot_recipe_red@resources0.rb_red_slot": {
    "size": [25, 25],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$background_image_control_name",
        "resolve_sibling_scope": true,
        "source_property_name": "(#container_item_background = 5)",
        "target_property_name": "#visible"
      }
    ]
  },

  /* Pages */

  "recipe_book_page_content_panel": {
    "type": "panel",
    "size": [128, "100%"],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle"
  },

  "left_button@toggle_button_ui.toggle_button": {
    "size": [11, 17],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "$toggle_index": 0,
    "ignored": "$ignore_left_button",
    "$checked_toggle_bg": "resources0.rb_left_button_default",
    "$unchecked_toggle_bg": "resources0.rb_left_button_default",
    "$checked_hover_toggle_bg": "resources0.rb_left_button_hover",
    "$unchecked_hover_toggle_bg": "resources0.rb_left_button_hover",
    "$checked_locked_toggle_bg": "",
    "$unchecked_locked_toggle_bg": "",
    "$controller_mapping_from_id": "button.menu_alternate_tab_left",
    "$controller_mapping_to_id": "button.menu_select"
  },

  "right_button@toggle_button_ui.toggle_button": {
    "size": [11, 17],
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "ignored": "$ignore_right_button",
    "$toggle_index": 1,
    "$checked_toggle_bg": "resources0.rb_right_button_default",
    "$unchecked_toggle_bg": "resources0.rb_right_button_default",
    "$checked_hover_toggle_bg": "resources0.rb_right_button_hover",
    "$unchecked_hover_toggle_bg": "resources0.rb_right_button_hover",
    "$checked_locked_toggle_bg": "",
    "$unchecked_locked_toggle_bg": "",
    "$use_controller_mapping": true,
    "$controller_mapping_from_id": "button.menu_alternate_tab_left",
    "$controller_mapping_to_id": "button.menu_select"
  },

  "page_content_panel": {
    "type": "panel",
    "$ignore_left_button|default": false,
    "$ignore_right_button|default": false,
    "$ignore_page_count|default": false,
    "$page_current_number|default": "1",
    "$page_total_number|default": "1",
    "$left_button_name|default": "",
    "$right_control_name|default": "",
    "$left_control_name|default": "",
    "$right_button_name|default": "",
    "$right_binding|default": [],
    "$left_binding|default": [],
    "controls": [
      {
        "recipe_book_scroll_panel": {
          "type": "panel",
          "size": [128, 100],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [0, 30],
          "layer": 10,
          "controls": [
            { "rb@recipes_ui.recipe_book_scroll_panel": { "offset": [0, 0], "$scrollbox_visible": false, "clips_children": true, "$scroll_view_port_offset": "$scroll_position" } }
          ]
        }
      },
      {
        "page_buttons_panel": {
          "type": "panel",
          "size": [65, 17],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "$toggle_type_panel": "",
          "offset": [0, -12],
          "controls": [
            {
              "left_button@recipes_ui.left_button": {
                "$focus_id": "page_arrow_left",
                "$toggle_button_name": "$left_button_name",
                "$toggle_control_name": "$left_control_name"
              }
            },
            { "page@page_current_number": {} },
            {
              "right_button@recipes_ui.right_button": {
                "$focus_id": "page_arrow_right",
                "$toggle_button_name": "$right_button_name",
                "$toggle_control_name": "$right_control_name"
              }
            },
            {
              "gamepadhelpers_panel": {
                "type": "panel",
                "controls": [
                  {
                    "gamepad_helper_left_trigger@gamepadhelper.gamepad_helper_left_trigger": {
                      "ignored": "$ignore_left_button",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "offset": [-26, -1]
                      
                    }
                  },
                  {
                    "gamepad_helper_right_trigger@gamepadhelper.gamepad_helper_right_trigger": {
                      "ignored": "$ignore_right_button",
                      "anchor_from": "right_middle",
                      "anchor_to": "right_middle",
                      "offset": [28, -1]
                    }
                  }
                ],
                "bindings": [
                  {
                    "ignored": "$render_gamepadhelpers",
                    "binding_name": "#gamepad_helper_visible",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "page_current_number": {
    "type": "label",
    "text": "($page_current_number + '/' + $page_total_number)",
    "shadow": true,
    "visible": "(not $ignore_page_count)",
    "anchor_from": "center",
    "anchor_to": "center",
    "layer": 10/* ,
    "bindings": [
      {
        "binding_name": "$binD",
        "binding_type": "collection",
        "binding_collection_name": "recipe_book",
        "binding_name_override": "#visible"
      }
    ] */
  },

  "recipe_book_1page_content_panel@recipe_book_page_content_panel": {
    "$scroll_position": [ 0, 0 ],
