  "right_button@toggle_button_mcjava.toggle_button": {
    "size": [11, 17],
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "ignored": "$ignore_right_button",
    "$toggle_index": 1,
    "$checked_toggle_bg": "resources0.rb_right_button_default",
