          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [0, 40],
          "controls": [
            { "console_title@command_block_mcjava.base_label": { "$text": "Relative Position" } },
            {
              "size_panel@structure_block_mcjava.grid_axis_selection_panel": {
                "size": [242, 22],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "$axis_selection_panel_label": "structure_block.size",
                "$grid_axis_collection_name": "save_offset_grid",
                "$grid_axis_edit_box_name": "#save_offset_edit_box"
              }
            },
            { "space@screen_mcjava.vspace": { "size": [0, 8] } },
            { "console_title@command_block_mcjava.base_label": { "$text": "Structure Size" } },
            {
              "offset_panel@structure_block_mcjava.grid_axis_selection_panel": {
                "size": [242, 22],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "$axis_selection_panel_label": "structure_block.offset",
                "$grid_axis_collection_name": "save_size_grid",
