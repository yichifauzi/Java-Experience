/* By @FMarquesPTO. All rights reserved. */
{
  "namespace": "container_ui",

  "container_panel": {
    "type": "panel",
    "$container_size|default": [176, 166],
    "$specified_content|default": "",
    "$inv_grid_panel|default": "container_ui.inventory_grid_panel",
    "$hotbar_grid_panel|default": "container_ui.hotbar_grid",
    "controls": [
      { "container_gamepad_helpers@gamepadhelper.container_gamepad_helpers": {} },
      { "invisible_exit_bg@container_ui.invisible_exit_bg": {} },
      {
        "root_panel@common.root_panel": {
          "size": "$container_size",
          "layer": 1,
          "controls": [
            { "close_button@container_ui.close_button": {} },
            { "bg@$container_bg": {} },
            {
              "screen_inventory": {
                "type": "panel",
                "layer": 2,
                "controls": [
                  { "$specified_content@$specified_content": {} },
                  { "inventory_grid_panel@$inv_grid_panel": {} },
                  { "hotbar_grid@$hotbar_grid_panel": {} },
                  { "inventory_take_progress_icon_button@container_ui.inventory_take_progress_icon_button": {} }
                ]
              }
            },
            { "inventory_selected_icon_button@container_ui.inventory_selected_icon_button": {} },
            { "gamepad_cursor@container_ui.gamepad_cursor_button": {} }
          ]
        }
      }
    ]
  },

  "close_button": {
    "type": "button",
    "size": [19, 19],
    "layer": 10,
    "offset": [23, 1],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "locked_control": "",
    "default_control": "default",
    "hover_control": "hover",
    "pressed_control": "pressed",
    "sound_name": "random.click",
    "sound_volume": 1.0,
    "sound_pitch": 1.0,
    "$button_tts_name|default": "",
    "$button_tts_header|default": "accessibility.button.close",
    "$tts_section_header|default": "",
    "tts_name": "$button_tts_name",
    "tts_control_header": "$button_tts_header",
    "tts_section_header": "$tts_section_header",
    "ignored": "($container_exit_way = invisible)",
    "bindings": [
      {
        "ignored": "$render_touch_close_btn",
        "binding_name": "#using_touch",
        "binding_name_override": "#visible"
      },
      {
        "binding_name": "#tts_enabled",
        "binding_name_override": "#focus_enabled"
      }
    ],
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.menu_exit",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.menu_exit",
        "mapping_type": "focused"
      }
    ],
    "controls": [
      { "default@resources0.close_button_default": {} },
      { "hover@resources0.close_button_hover": {} },
      { "pressed@resources0.close_button_hover": {} }
    ]
  },

  "invisible_exit_bg": {
    "type": "input_panel",
    "size": ["30%", "10% + 20px"],
    "focus_enabled": false,
    "ignored": "($container_exit_way = visible_button)",
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.menu_exit",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.menu_exit",
        "mapping_type": "focused"
      }
    ],
    "bindings": [
      {
        "binding_name": "#using_touch",
        "binding_name_override": "#visible"
      }
    ]
  },
  
  "title": {
    "type": "label",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "text": "$container_title",
    "color": "$container_text_color",
    "layer": 3
  },

  /* Slot */

  "container_slot": {
    "type": "input_panel",
    "$focus_id|default": "",
    "$focus_override_down|default": "",
    "$focus_override_up|default": "",
    "$focus_override_left|default": "",
    "$focus_override_right|default": "",
    "focus_identifier": "$focus_id",
    "focus_change_down": "$focus_override_down",
    "focus_change_up": "$focus_override_up",
    "focus_change_left": "$focus_override_left",
    "focus_change_right": "$focus_override_right",
    "focus_enabled": true,
    "focus_wrap_enabled": false,
    "focus_magnet_enabled": true,
    "size": [18, 18],
    "$cell_size|default": ["100%", "100%"],
    "$background_images|default": "common.empty_panel",
    "$background_image_control_name|default": "bg",
    "$item_renderer_panel_size|default": ["100%", "100%"],
    "$item_renderer|default": "common.item_renderer",
    "$item_renderer_size|default": [16, 16],
    "$item_renderer_offset|default": [0, 0],
    "$cell_overlay_ref|default": "common.empty_panel",
    "$button_ref|default": "container_ui.container_slot_button_prototype",
    "$highlight_slot_required|default": true,
    "$stack_count_required|default": true,
    "$durability_bar_required|default": true,
    "$durability_bar_offset": [0, 5.5],
    "controls": [
      {
        "item_cell": {
          "type": "panel",
          "size": "$cell_size",
          "controls": [
            { "$background_image_control_name@$background_images": { "layer": 1 } },
            {
              "item": {
                "type": "panel",
                "size": "$item_renderer_panel_size",
                "controls": [
                  { "stack_count_label@container_ui.stack_count_label": { "layer": 8 } },
                  {
                    "$item_renderer@$item_renderer": {
                      "type": "custom",
                      "renderer": "inventory_item_renderer",
                      "$item_renderer_binding_condition|default": "none",
                      "size": "$item_renderer_size",
                      "offset": "$item_renderer_offset",
                      "anchor_to": "center",
                      "anchor_from": "center",
                      "layer": 5
                    }
                  }
                ]
              }
            },
            { "durability_bar@container_ui.durability_bar": { "layer": 6 } }
          ]
        }
      },
      { "item_cell_overlay_ref@$cell_overlay_ref": { "layer": 2 } },
      { "item_button_ref@$button_ref": { "layer": 4 } }
    ]
  },

  "durability_bar": {
    "ignored": "(not $durability_bar_required)",
    "type": "custom",
    "renderer": "progress_bar_renderer",
    "layer": 10,
    "$durability_bar_size|default": [ 12, 1 ],
    "$durability_bar_offset|default": [ 0, 5 ],
    "offset": "$durability_bar_offset",
    "size": "$durability_bar_size",
    "property_bag": {
      "is_durability": true,
      "round_value": true
    },
    "bindings": [
      {
        "binding_name": "#item_durability_visible",
        "binding_name_override": "#progress_bar_visible",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      },
      {
        "binding_name": "#item_durability_total_amount",
        "binding_name_override": "#progress_bar_total_amount",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      },
      {
        "binding_name": "#item_durability_current_amount",
        "binding_name_override": "#progress_bar_current_amount",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      }
    ]
  },

  "stack_count_label": {
    "type": "label",
    "text": "$stack_count_binding_name",
    "color": "$item_stack_count_text_color",
    "shadow": true,
    "anchor_to": "bottom_right",
    "anchor_from": "bottom_right",
    "offset": [0, 1],
    "layer": 7,
    "$stack_count_binding_name|default": "#inventory_stack_count",
    "$stack_count_binding_condition|default": "none",
    "ignored": "(not $stack_count_required)",
    "bindings": [
      {
        "binding_name": "$stack_count_binding_name",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "$stack_count_binding_condition"
      }
    ]
  },

  "container_slot_button_prototype": {
    "type": "button",
    "$focus_id|default": "",
    "$focus_override_down|default": "",
    "$focus_override_up|default": "",
    "$focus_override_left|default": "",
    "$focus_override_right|default": "",
    "$focus_id_binding_type|default": "collection",
    "$focus_down_binding_type|default": "collection",
    "$focus_up_binding_type|default": "collection",
    "$focus_left_binding_type|default": "collection",
    "$focus_right_binding_type|default": "collection",
    "focus_identifier": "$focus_id",
    "focus_change_down": "$focus_override_down",
    "focus_change_up": "$focus_override_up",
    "focus_change_left": "$focus_override_left",
    "focus_change_right": "$focus_override_right",
    "focus_enabled": true,
    "focus_wrap_enabled": false,
    "focus_magnet_enabled": true,
    "default_focus_precedence": 1,
    "$highlight_hover_control|default": "container_ui.highlight_hover_slot_panel",
    "$highlight_pressed_control|default": "container_ui.highlight_pressed_slot_panel",
    "hover_control": "hover",
    "pressed_control": "pressed",
    "layer": 1,
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      },
      {
        "binding_type": "$focus_id_binding_type",
        "binding_name": "#focus_identifier",
        "binding_name_override": "#focus_identifier",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      },
      {
        "binding_type": "$focus_down_binding_type",
        "binding_name": "#focus_change_down",
        "binding_name_override": "#focus_change_down",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      },
      {
        "binding_type": "$focus_up_binding_type",
        "binding_name": "#focus_change_up",
        "binding_name_override": "#focus_change_up",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      },
      {
        "binding_type": "$focus_left_binding_type",
        "binding_name": "#focus_change_left",
        "binding_name_override": "#focus_change_left",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      },
      {
        "binding_type": "$focus_right_binding_type",
        "binding_name": "#focus_change_right",
        "binding_name_override": "#focus_change_right",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "once"
      }
    ],
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.container_take_all_place_all",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.container_take_all_place_all",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_secondary_select",
        "to_button_id": "button.container_take_half_place_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.container_take_half_place_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_auto_place",
        "to_button_id": "button.container_auto_place",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.container_auto_place",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_inventory_drop",
        "to_button_id": "button.drop_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_inventory_drop_all",
        "to_button_id": "button.drop_all",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.coalesce_stack",
        "mapping_type": "double_pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.coalesce_stack",
        "mapping_type": "double_pressed"
      },
      {
        "to_button_id": "button.shape_drawing",
        "mapping_type": "pressed"
      },
      {
        "to_button_id": "button.container_slot_hovered",
        "mapping_type": "pressed"
      }
    ],
    "controls": [
      { "hover@$highlight_hover_control": {} },
      { "pressed@$highlight_pressed_control": {} }
    ]
  },

  "no_coalesce_container_slot_button@container_ui.container_slot_button_prototype": {
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.container_take_all_place_all",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.container_take_all_place_all",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_secondary_select",
        "to_button_id": "button.container_take_half_place_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.container_take_half_place_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_auto_place",
        "to_button_id": "button.container_auto_place",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.container_auto_place",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_inventory_drop",
        "to_button_id": "button.drop_one",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_inventory_drop_all",
        "to_button_id": "button.drop_all",
        "mapping_type": "pressed"
      },
      {
        "to_button_id": "button.shape_drawing",
        "mapping_type": "pressed"
      },
      {
        "to_button_id": "button.container_slot_hovered",
        "mapping_type": "pressed"
      }
    ]
  },

  /* Slot Highlight */

  "highlight_hover_slot_panel": {
    "type": "panel",
    "layer": 4,
    "controls": [
      { "highlight_slot_image@container_ui.highlight_slot_image": {} },
      {
        "hover_text@container_ui.hover_text": {
          "layer": 50,
          "$hover_text_binding_name|default": "#hover_text",
          "bindings": [
            {
              "binding_name": "$hover_text_binding_name",
              "binding_name_override": "#hover_text",
              "binding_type": "collection",
              "binding_collection_name": "$item_collection_name"
            }
          ]
        }
      }
    ]
  },

  "highlight_pressed_slot_panel": {
    "type": "panel",
    "layer": 4,
    "controls": [
      {
        "highlight_slot_image@container_ui.highlight_slot_image": {
          "bindings": [
            {
              "binding_name": "(not #using_touch)",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "hover_text@container_ui.hover_text": {
          "layer": 50,
          "$hover_text_binding_name|default": "#hover_text",
          "bindings": [
            {
              "binding_name": "$hover_text_binding_name",
              "binding_name_override": "#hover_text",
              "binding_type": "collection",
              "binding_collection_name": "$item_collection_name"
            }
          ]
        }
      }
    ]
  },

  /* Item Tooltip/Hover Text */

  "hover_text": {
    "type": "button",
    "size": [0, 0],
    "layer": -10,
    "follows_cursor": true,
    "always_handle_pointer": true,
    "anchor_from": "center",
    "anchor_to": "center",
    "allow_clipping": false,
    "consume_hover_events": false,
    "$hover_text_bindings|default": [
      {
        "binding_name": "#hover_text",
        "binding_name_override": "#hover_text",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      }
    ],
    "$hover_text_parent_bindings|default": [
      {
        "binding_name": "(not (#hover_text = ''))",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name",
        "binding_name_override": "#visible"
      }
    ],
    "controls": [
      {
        "hover_text": {
          "type": "image",
          "texture": "src/core/assets/fmarquespto/tooltip",
          "nineslice_size": 3,
          "size": ["100%c + 8px", "100%c + 6px"],
          "offset": ["50%c + 12px", "50%c - 12px"],
          "layer": 70,
          "follows_cursor": true,
          "controls": [
            {
              "text": {
                "type": "label",
                "size": ["default", "default"],
                "text": "#hover_text",
                "shadow": true,
                "layer": 10,
                "bindings": "$hover_text_bindings"
              }
            }
          ],
          "bindings": "$hover_text_parent_bindings"
        }
      }
    ]
  },

  "highlight_slot_image": {
    "type": "image",
    "texture": "src/core/assets/fmarquespto/white",
    "size": [16, 16],
    "color": "$highlight_slot_bg_color",
    "ignored": "(not $highlight_slot_required)",
    "alpha": "$highlight_slot_bg_alpha"
  },

  /* Inventory/Hotbar Slot Grid */

  "grid_item_for_inventory@container_ui.container_slot": {
    "$item_collection_name": "inventory_items"
  },

  "grid_item_for_hotbar@container_ui.container_slot": {
    "$item_collection_name": "hotbar_items"
  },

  "inventory_grid_panel": {
    "type": "panel",
    "size": [162, 54],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": "$inv_offset",
    "$inv_offset|default": [0, -29],
    "$invlabel_offset|default": [1, -12],
    "$show_invlabel|default": true,
    "controls": [
      {
        "inventory_label": {
          "type": "label",
          "color": "$container_text_color",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": "$invlabel_offset",
          "visible": "$show_invlabel",
          "layer": 2,
          "text": "container.inventory"
        }
      },
      {
        "inventory_grid": {
          "type": "grid",
          "size": [162, 54],
          "grid_dimensions": [9, 3],
          "grid_item_template": "container_ui.grid_item_for_inventory",
          "collection_name": "inventory_items"
        }
      }
    ]
  },

  "hotbar_grid": {
    "type": "grid",
    "size": [ 162, 18 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": "$hotbar_offset",
    "grid_dimensions": [ 9, 1 ],
    "grid_item_template": "container_ui.grid_item_for_hotbar",
    "collection_name": "hotbar_items",
    "$hotbar_offset|default": [ 0, -7 ]
  },

  /* Flying Item */

  "inventory_take_progress_icon_button": {
    "type": "button",
    "locked_control": "",
    "default_control": "default",
    "hover_control": "hover",
    "pressed_control": "pressed",
    "layer": -10,
    "size": [ 16, 16 ],
    "follows_cursor": true,
    "sound_name": "random.click",
    "sound_volume": 1.0,
    "sound_pitch": 1.0,
    "always_handle_pointer": true,
    "controls": [
      { "default@container_ui.inventory_hold_icon": {} },
      { "hover@container_ui.inventory_hold_icon": {} },
      { "pressed@container_ui.inventory_hold_icon": {} }
    ]
  },

  "inventory_selected_icon_button": {
    "type": "button",
    "anchor_from": "center",
    "anchor_to": "center",
    "locked_control": "",
    "default_control": "default",
    "hover_control": "hover",
    "pressed_control": "pressed",
    "layer": -10,
    "size": [ 16, 16 ],
    "follows_cursor": true,
    "sound_name": "random.click",
    "sound_volume": 1.0,
    "sound_pitch": 1.0,
    "always_handle_pointer": true,
    "$durability_bar_required|default": true,
    "controls": [
      { "default@container_ui.inventory_icon_panel": {} },
      { "pressed@container_ui.inventory_icon_panel": {} },
      { "hover@container_ui.inventory_icon_panel": {} },
      { "durability_bar_grabbed@container_ui.durability_bar_grabbed": {} }
    ]
  },

  "inventory_hold_icon": {
    "type": "custom",
    "renderer": "progress_bar_renderer",
    "layer": 70,
    "size": [15, 3],
    "bindings": [
      {
        "binding_name": "#progress_bar_visible",
        "binding_condition": "always"
      },
      {
        "binding_name": "#progress_bar_total_amount",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#progress_bar_current_amount",
        "binding_condition": "always_when_visible"
      }
    ]
  },

  "inventory_icon_panel": {
    "type": "panel",
    "controls": [
      { "selected_item_icon@container_ui.inventory_selected_icon": {} },
      { "selected_stack_size_text@container_ui.inventory_selected_stack_size_text": {} },
      {
        "hover_text@common.hover_text": {
          "layer": 59,
          "bindings": [
            {
              "binding_name": "#using_touch",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  "inventory_selected_icon@common.item_renderer": {
    "layer": 50,
    "size": [ 16, 16 ],
    "offset": [0, 0],
    "bindings": [
      {
        "binding_name": "#inventory_selected_item",
        "binding_name_override": "#item_id_aux",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#inventory_selected_item_charged_item",
        "binding_name_override": "#charged_item",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#inventory_selected_item_color",
        "binding_name_override": "#item_custom_color",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#inventory_selected_banner_patterns",
        "binding_name_override": "#banner_patterns",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#inventory_selected_banner_colors",
        "binding_name_override": "#banner_colors",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#inventory_selected_banner_type",
        "binding_name_override": "#banner_type",
        "binding_condition": "always_when_visible"
      }
    ]
  },

  "inventory_selected_stack_size_text": {
    "type": "label",
    "offset": [0, 2],
    "anchor_to": "bottom_right",
    "anchor_from": "bottom_right",
    "layer": 53,
    "shadow": true,
    "color": "$item_stack_count_text_color",
    "text": "#inventory_selected_item_stack_count",
    "bindings": [
      {
        "binding_name": "#inventory_selected_item_stack_count",
        "binding_condition": "always_when_visible"
      }
    ]
  },

  "durability_bar_grabbed@durability_bar": {
    "layer": 61,
    "offset": [ "-10% + 1px", 5.5 ],
    "size": [ 12, 1 ],
    "bindings": [
      {
        "binding_name": "#selected_item_durability_visible",
        "binding_name_override": "#progress_bar_visible",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#selected_item_durability_total_amount",
        "binding_name_override": "#progress_bar_total_amount",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_name": "#selected_item_durability_current_amount",
        "binding_name_override": "#progress_bar_current_amount",
        "binding_condition": "always_when_visible"
      }
    ]
  },

  /* Controller Pointer */

  "gamepad_cursor_button": {
    "type": "button",
    "visible": "$render_gamepadhelpers",
    "anchor_from": "center",
    "anchor_to": "center",
    "locked_control": "",
    "default_control": "default",
    "hover_control": "hover",
    "pressed_control": "pressed",
    "layer": -10,
    "size": [ 16, 16 ],
    "follows_cursor": true,
    "always_handle_pointer": true,
    "controls": [
      { "default@gamepad_cursor_image": {} },
      { "pressed@gamepad_cursor_image": {} },
      { "hover@gamepad_cursor_image": {} }
    ]
  },

  "gamepad_cursor_image": {
    "type": "image",
    "texture": "src/core/assets/fmarquespto/pointer",
    "size": [ 14, 14 ],
    "layer": 60,
    "follows_cursor": true,
    "bindings": [
      {
        "ignored": "$render_gamepadhelpers",
        "binding_name": "#gamepad_cursor_visible",
        "binding_name_override": "#visible",
        "binding_condition": "always"
      }
    ]
  },

  /* Container Scrolls */

  "container_scroll_panel@scrolling_ui.scrolling_panel": {
    "$scrolling_content|default": "container_ui.container_grid",
    "$scroll_clip_path": true,
    "$scrolling_panel_base_button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_secondary_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.container_reset_held",
        "mapping_type": "pressed"
      }
    ]
  },

  "container_grid": {
    "type": "grid",
    "grid_rescaling_type": "horizontal",
    "size": [ "100%", "default" ],
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "$item_collection_name|default": "inventory_items",
    "$grid_item_template|default": "container_ui.container_slot",
    "collection_name": "$item_collection_name",
    "grid_item_template": "$grid_item_template",
    "bindings": [
      {
        "binding_name": "#collection_total_items",
        "binding_name_override": "#maximum_grid_items",
        "binding_condition": "visible",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      }
    ]
  }
}